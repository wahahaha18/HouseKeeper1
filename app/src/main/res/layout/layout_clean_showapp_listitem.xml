        <!--android:id="@+id/tv_appPackageName"-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:textSize="16sp"-->
        <!--android:text="com.zxzq.housekeeper"-->
        <!--android:layout_toRightOf="@id/iv_appIcon"-->
        <!--android:layout_alignBottom="@id/iv_appIcon"-->
        <!--android:layout_marginLeft="5dp"-->

        <!--android:layout_below="@id/tv_appLabel"-->
        <!--android:gravity="center_vertical"-->
        <!--/>-->
    <TextView
        android:id="@+id/tv_clean_item_appVersion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="21.00MB"
        android:layout_alignParentRight="true"
        android:layout_marginRight="15dp"
        android:layout_marginTop="20dp"/>

</RelativeLayout>
